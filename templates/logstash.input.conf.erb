input {
<% cnf=@conf_file.gsub(/^\d+-/, '') -%>
  <%=cnf-%> {
<% @contents['input'].each do |line| -%>
    <%= line %>
<% end -%>
  }
}
<% if @contents['filter'] -%>
filter {
<%@contents['filter'].each do |line| -%>
  <%=line%>
<%end-%>
}
<%end-%>
<% if @contents['output'] -%>
output {
  <%=@contents['output'].keys[0]%> {
    index => "<%=@contents['output'][@contents['output'].keys[0]]%>"
  }
}
<%end-%>

